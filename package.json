{
  "name": "short-source",
  "version": "0.1.0",
  "private": true,
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "dependencies": {
    "expo": "^27.0.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "react": "^16.3.2",
    "react-art": "^16.3.2",
    "react-autobind": "^1.0.6",
    "react-dom": "^16.3.2",
    "react-native": "^0.55.4",
    "react-native-web": "^0.6.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "recharts": "^1.0.0-beta.10"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-preset-expo": "^4.0.0",
    "chronokinesis": "^1.3.2",
    "flow-bin": "^0.72.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jest-expo": "^27.0.1",
    "lint-staged": "^7.1.0",
    "nodemon": "^1.17.4",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.12.1",
    "react-native-scripts": "^1.14.0",
    "react-test-renderer": "^16.3.2",
    "rename-cli": "^5.1.0"
  },
  "scripts": {
    "postinstall": "cd functions;npm install;cd ..",
    "clean": "rimraf build",
    "build": "react-scripts build",
    "postbuild": "./node_modules/.bin/rename build/index.html build/app.html",
    "predeploy": "npm run build",
    "deploy": "firebase deploy",
    "dev:android": "react-native-scripts android",
    "dev:ios": "react-native-scripts ios",
    "dev:native": "react-native-scripts start",
    "dev:web": "react-scripts start",
    "dev": "npm-run-all -p serve dev:web",
    "serve": "firebase serve --only functions",
    "tdd": "jest --coverage --watch",
    "test": "jest --coverage",
    "eject:native": "react-native-scripts eject",
    "eject": "react-scripts eject",
    "precommit": "lint-staged"
  },
  "jest": {
    "projects": [
      {
        "displayName": "app-web",
        "testMatch": [
          "<rootDir>/src/**/*.test.js"
        ],
        "moduleNameMapper": {
          "react-native": "<rootDir>/node_modules/react-native-web"
        }
      },
      {
        "displayName": "app-native",
        "testMatch": [
          "<rootDir>/*.test.js"
        ],
        "preset": "jest-expo"
      },
      {
        "displayName": "functions",
        "testMatch": [
          "<rootDir>/functions/**/*.test.js"
        ]
      }
    ]
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:5000/moniker-app/us-central1/",
      "pathRewrite": {
        "^/api": ""
      }
    }
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "always"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
